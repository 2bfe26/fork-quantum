{
    "extends": "airbnb",
    "env": {
        "jest": true
    },
    "parser": "babel-eslint",
    "rules": {
        "strict": 0,
        "import/no-extraneous-dependencies": 0,
        "jsx-a11y/label-has-for": [ 2, {
            "components": [ "Label" ],
            "required": {
                "some": [ "nesting", "id" ]
            },
            "allowChildren": false
        }]
    },
    "globals": {
        "window": true,
        "document": true
    }
}
