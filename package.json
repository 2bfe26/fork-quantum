{
  "name": "@catho/quantum",
  "version": "0.0.0-development",
  "description": "Catho react components",
  "private": false,
  "sideEffects": false,
  "scripts": {
    "test": "jest",
    "test:components": "yarn test ./components",
    "test:coverage": "yarn test:components --coverage",
    "build": "cross-env NODE_ENV=production babel ./components --out-dir dist --copy-files",
    "clean:files:build": "rm ./dist/**/*.test.jsx ./dist/**/*/*.test.jsx",
    "clean:directories:build": "rm -rf ./dist/**/__snapshots__ ./dist/**/*/__snapshots__",
    "clean:build": "rm -rf ./dist",
    "postbuild": "yarn clean:directories:build && yarn clean:files:build && yarn copy-files",
    "copy-files": "babel-node ./scripts/copy-files.js",
    "prebuild": "yarn clean:build",
    "commitizen": "git-cz",
    "precommit": "lint-staged",
    "lint:components": "yarn lint ./components",
    "lint:stories": "yarn lint ./stories",
    "lint:js": "yarn lint ./*.js",
    "lint:all": "yarn lint:components && yarn lint:stories && yarn lint:js",
    "lint": "eslint --ext js,jsx --quiet",
    "prepush": "yarn lint:all && yarn test:components && yarn gh-pages",
    "storybook": "start-storybook -s static",
    "build-storybook": "build-storybook",
    "format": "prettier --write ./**/*.{js,jsx,json}",
    "gh-pages:clean": "rm -rf docs",
    "gh-pages:build": "build-storybook -o docs",
    "gh-pages:update": "./scripts/update-docs.sh",
    "gh-pages": "yarn gh-pages:clean && yarn gh-pages:build && yarn gh-pages:update",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https:git@github.com:catho/quantum.git"
  },
  "keywords": [
    "catho",
    "react",
    "components",
    "style",
    "guide",
    "quantum"
  ],
  "author": "Catho",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-links": "^4.1.4",
    "@storybook/addon-options": "^4.1.4",
    "@storybook/addons": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.0-beta.6",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-modern-regexp": "^0.0.6",
    "commitizen": "^3.0.7",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "2.1.0",
    "dotenv-webpack": "^1.6.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-diff": "^24.0.0",
    "jest-styled-components": "^6.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-cookies": "^0.1.0",
    "react-dom": "^16.7.0",
    "react-test-renderer": "^16.7.0",
    "regenerator-runtime": "^0.13.1",
    "strip-ansi": "^5.0.0",
    "styled-components": "^4.1.3",
    "semantic-release": "^15.13.3"
  },
  "dependencies": {
    "@catho-private/quantum-storybook-ui": "^0.1.0",
    "@material-ui/core": "^3.8.2",
    "@material-ui/icons": "^3.0.2",
    "babel-polyfill": "^6.26.0",
    "datalist-polyfill": "^1.23.1",
    "downshift": "^3.1.10",
    "prop-types": "^15.6.2",
    "rc-slider": "^8.6.4",
    "react-content-loader": "^3.4.2",
    "react-image": "^1.5.1",
    "react-text-mask": "^5.4.3"
  },
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "styled-components": ">= 4"
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "yarn format",
      "git add"
    ]
  },
  "jest": {
    "clearMocks": true,
    "coverageDirectory": "coverage",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileMock.js",
      "\\.(css|less)$": "<rootDir>/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/jest-config.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "restricted"
  }
}
