{
  "name": "@cathodevel/quantum",
  "version": "0.12.2",
  "description": "Catho react components",
  "main": "./dist/index.js",
  "scripts": {
    "test": "jest",
    "analyze-bundle": "webpack --json | webpack-bundle-size-analyzer",
    "coverage": "jest --coverage",
    "build": "cross-env NODE_ENV=production webpack",
    "precommit": "lint-staged",
    "prepush": "yarn lint && yarn test",
    "storybook": "cross-env NODE_ENV=storybook start-storybook -p 6006 -s static",
    "build-storybook": "build-storybook",
    "format": "prettier --write ./**/*.{js,jsx,json}",
    "lint": "eslint ./components ./.storybook ./stories ./*.js --ext js,jsx --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.devel:frontend-platform/quantum.git"
  },
  "keywords": [
    "catho",
    "react",
    "components",
    "style",
    "guide",
    "quantum"
  ],
  "author": "Catho",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-links": "^4.1.4",
    "@storybook/addon-options": "^4.1.4",
    "@storybook/addons": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.0-beta.6",
    "babel-plugin-react-docgen": "^2.0.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-modern-regexp": "^0.0.6",
    "compression-webpack-plugin": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "dotenv-webpack": "^1.6.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-cookies": "^0.1.0",
    "react-dom": "^16.7.0",
    "react-test-renderer": "^16.7.0",
    "regenerator-runtime": "^0.13.1",
    "style-loader": "^0.23.1",
    "styled-components": "^4.1.3",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@material-ui/core": "^3.8.2",
    "@material-ui/icons": "^3.0.2",
    "babel-polyfill": "^6.26.0",
    "datalist-polyfill": "^1.23.1",
    "downshift": "^3.1.10",
    "prop-types": "^15.6.2",
    "rc-slider": "^8.6.4",
    "react-content-loader": "^3.4.2",
    "react-image": "^1.5.1",
    "react-text-mask": "^5.4.3"
  },
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "styled-components": ">= 4"
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "yarn format",
      "git add"
    ]
  },
  "jest": {
    "clearMocks": true,
    "coverageDirectory": "coverage",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileMock.js",
      "\\.(css|less)$": "<rootDir>/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/jest-config.js"
  }
}
